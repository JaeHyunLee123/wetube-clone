{
  "name": "wetube_clone",
  "version": "1.0.0",
  "engines": {
    "npm": ">=8.0.0 <9.0.0",
    "node": ">=16.0.0 <17.0.0"
  },
  "description": "wetube clone with node.js, express, mongodb and ES6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaeHyunLee123/wetube_clone.git"
  },
  "scripts": {
    "start": "npm run build && node build/init.js",
    "build": "npm run build:server && npm run build:assets",
    "build:server": "babel src -d build --ignore src/frontend",
    "build:assets": "webpack --mode=production",
    "dev:server": "nodemon",
    "dev:assets": "webpack --mode=development --watch"
  },
  "keywords": [
    "wetube"
  ],
  "author": "JaehyunLee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JaeHyunLee123/wetube_clone/issues"
  },
  "homepage": "https://github.com/JaeHyunLee123/wetube_clone#readme",
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "@prettier/plugin-pug": "^2.4.1",
    "aws-sdk": "^2.1321.0",
    "babel-loader": "^9.1.2",
    "bcrypt": "^5.1.0",
    "connect-mongo": "^4.6.0",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "install": "^0.13.0",
    "mini-css-extract-plugin": "^2.7.2",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.20",
    "pug": "^3.0.2",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "prettier": "^2.8.4"
  }
}
